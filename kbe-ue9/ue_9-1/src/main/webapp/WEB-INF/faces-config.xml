<?xml version="1.0"?>
<faces-config version="2.2"
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">


<application>
	<locale-config>
	    <default-locale>de</default-locale>
	</locale-config>
	<message-bundle>
		de.htw_berlin.ai_bachelor.kbe.checklist.messages_de
	</message-bundle>
</application>


<navigation-rule>
	<from-view-id>editToDoList.xhtml</from-view-id>
	<navigation-case>
		<from-action>#{list.save}</from-action>
		<to-view-id>/editToDoList.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>

<navigation-rule>
	<from-view-id>editInterval.xhtml</from-view-id>
	<navigation-case>
		<from-action>#{priority.save}</from-action>
		<to-view-id>/editToDoList.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>

</faces-config>

